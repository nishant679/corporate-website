{
	"name": "corporate-website",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "node ./bin/www",
		"dev:server": "nodemon --ignore './client/' --ignore './public/' ./bin/www ",
		"dev:client": "cd client && npm start",
		"dev-client-install": "cd client && npm install",
		"dev-server-install": "npm install",
		"install-all": "concurrently \"npm install \" \"cd client && npm install\" ",
		"dev": "npm-run-all --parallel dev:server dev:client"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"concurrently": "^4.1.2",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"dotenv": "^8.1.0",
		"express": "~4.16.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.7.1",
		"morgan": "~1.9.1",
		"multer": "^1.4.2",
		"node-sass": "^4.12.0",
		"react": "^16.10.1",
		"react-scripts": "^3.1.2"
	},
	"devDependencies": {
		"nodemon": "^1.19.2",
		"npm-run-all": "^4.1.5"
	}
}
